    "signal_requirements": {
      "min_probability": 0.65,
      "min_confidence": 0.55
    }
  },
  
  "tensorflow": {
    "features": {
      "market_state": [
        "obi",
        "wpi", 
        "vpin",
        "funding_rate",
        "oi_change_pct",
